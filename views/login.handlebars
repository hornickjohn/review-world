<h1>Login</h1>

<form id="loginForm">
    <input type="text" id="loginEmail" placeholder="email">
    {{#if userData.showname}}
        {{userData.email}}
    <input type="password" id="loginPw">
    {{userData.password}}
    {{/if}}
    <button>Login</button>
</form>

<script src="/js/login.js"></script>